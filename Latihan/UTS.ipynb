    "import json\n",
    "\n",
    "def soal_1():\n",
    "    reverse_sorted = [{'name': perusahaan['name']} for perusahaan in sorted(company_detail_list, key=lambda x: x['name'], reverse=True)]\n",
    "    return reverse_sorted\n",
    "\n",
    "print(\"Hasil Soal 1:\")\n",
    "print(json.dumps(soal_1(), indent=4))"
